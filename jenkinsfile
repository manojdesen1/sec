pipeline {
  agent any

  tools {
    jdk 'Java'
    maven 'Maven'
  }
  
  
  
stages {
    stage('Git checkout'){
      steps {
        git branch: '{master}',
        url: 'https://github.com/manojdesen1/sec.git'
      }
    }
    stage('Maven build'){
      steps {
        sh 'mvn clean install'
      }
    }
  }
}
